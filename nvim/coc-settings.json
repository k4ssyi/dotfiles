{
  "coc.preferences.extensionUpdateCheck":"weekly",
  "suggest.autoTrigger": "always",
  "suggest.minTriggerInputLength":3,
  "suggest.noselect":true,
  "suggest.floatConfig":{"border":false,"rounded":true,"winblend": 3},
  "diagnostic.floatConfig":{"border":true,"winblend": 7},
  "signature.floatConfig":{"winblend": 5},
  "hover.floatConfig":{"border":true,"winblend": 7},
  "eslint.enable":true,
  "astro.enable": true,
  "diagnostic.separateRelatedInformationAsDiagnostics":true,
  "diagnostic.virtualText":false,
  "diagnostic.virtualTextCurrentLineOnly":false,
  "diagnostic.virtualTextLevel":"warning",
  "diagnostic.virtualTextPrefix":"◆",
  "diagnostic.warningSign":"◆",
  "diagnostic.enableHighlightLineNumber":false,
  "typescript.showUnused":true,
  "typescript.showDeprecated":false,
  "javascript.showDeprecated":false,
  "typescript.preferences.quoteStyle":"single",
  "javascript.preferences.quoteStyle":"single",
  "typescript.format.enable":false,
  "javascript.format.enable":false,
  "typescript.autoClosingTags":false,
  "css.completion.completePropertyWithSemicolon":false,
  "scss.completion.completePropertyWithSemicolon":false,
  "yank.enableCompletion":false,
  "suggest.completionItemKindLabels": {
    "function": "\uf794",
    "method": "\uf6a6",
    "variable": "\uf71b",
    "constant": "\uf8ff",
    "struct": "\ufb44",
    "class": "\uf0e8",
    "interface": "\ufa52",
    "text": "\ue612",
    "enum": "\uf435",
    "enumMember": "\uf02b",
    "color": "\ue22b",
    "property": "\ufab6",
    "field": "\uf93d",
    "unit": "\uf475",
    "file": "\uf471",
    "value": "\uf8a3",
    "event": "\ufacd",
    "folder": "\uf115",
    "keyword": "\uf893",
    "snippet": "\uf64d",
    "operator": "\uf915",
    "reference": "\uf87a",
    "typeParameter": "\uf278",
    "default": "\uf29c"
  },
  "clangd.path": "~/.config/coc/extensions/coc-clangd-data/install/16.0.2/clangd_16.0.2/bin/clangd",
  "sumneko-lua.enableNvimLuaDev": true,
  "jedi.enable": true,
  "jedi.startupMessage": false,
  "jedi.executable.command": "jedi-language-server",
  "diagnostic-languageserver.filetypes": {
    "python": ["flake8"]
  },
  "diagnostic-languageserver.formatFiletypes": {
    "python": ["black"]
  },
  "diagnostic-languageserver.linters": {
    "flake8": {
      "command": "flake8",
      "debounce": 100,
      "args": [ "--format=%(row)d,%(col)d,%(code).1s,%(code)s: %(text)s", "-" ],
      "offsetLine": 0,
      "offsetColumn": 0,
      "sourceName": "flake8",
      "formatLines": 1,
      "formatPattern": [
        "(\\d+),(\\d+),([A-Z]),(.*)(\\r|\\n)*$",
        {
          "line": 1,
          "column": 2,
          "security": 3,
          "message": 4
        }
      ],
      "securities": {
        "W": "warning",
        "E": "error",
        "F": "error",
        "C": "error",
        "N": "error"
      }
    }
  },
  "diagnostic-languageserver.formatters": {
    "black": {
      "command": "black",
      "args": ["--quiet", "-"]
    }
  },
  "languageserver": {
  }
}

