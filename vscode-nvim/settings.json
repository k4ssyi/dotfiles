{
	"editor.renderWhitespace": "all",
	"editor.tabSize": 2,
	"editor.wordWrap": "on",
	"files.insertFinalNewline": true,
	"files.trimFinalNewlines": true,
	"editor.renderControlCharacters": true,
	"workbench.statusBar.visible": true,
	"emmet.showSuggestionsAsSnippets": true,
	"emmet.triggerExpansionOnTab": true,
	"emmet.variables": {
		"lang": "ja"
	},
	"workbench.iconTheme": "vscode-icons",
	"editor.bracketPairColorization.enabled": true,
	"files.autoSave": "afterDelay",
	"files.autoSaveDelay": 1500,
	"editor.formatOnSave": true,
	"editor.codeActionsOnSave": {
		"source.organizeImports.biome": "explicit"
	},
	"[typescript]": {
		"editor.defaultFormatter": "biomejs.biome"
	},
	"[typescriptreact]": {
		"editor.defaultFormatter": "biomejs.biome"
	},
	"editor.rename.enablePreview": false,
	"editor.guides.bracketPairs": true,
	"editor.suggest.snippetsPreventQuickSuggestions": false,
	"editor.copyWithSyntaxHighlighting": false,
	"editor.cursorSmoothCaretAnimation": "on",
	"editor.smoothScrolling": true,
	"workbench.list.smoothScrolling": true,
	"terminal.integrated.smoothScrolling": true,
	"editor.cursorBlinking": "smooth",
	"files.exclude": {
		"**/.git": false,
		"**/.venv/*": true,
		"**/.yarn/*": true,
		"**/*.pyc": true
	},
	"editor.fontSize": 13,
	"gitlens.codeLens.enabled": false,
	"terminal.integrated.fontSize": 14,
	"editor.unicodeHighlight.allowedCharacters": {
		"äº‹": true
	},
	"editor.unicodeHighlight.nonBasicASCII": false,
	"window.newWindowDimensions": "maximized",
	"security.workspace.trust.untrustedFiles": "prompt",
	"vscode-neovim.mouseSelectionStartVisualMode": true,
	"editor.scrollBeyondLastLine": false,
	"vscode-neovim.neovimInitVimPaths.darwin": "~/.config/vscode-nvim/init.lua",
	"diffEditor.renderSideBySide": true,
	"javascript.inlayHints.parameterNames.enabled": "literals",
	"editor.unicodeHighlight.ambiguousCharacters": false,
	"editor.rulers": [
		99,
		120
	],
	"settingsSync.ignoredSettings": [
		"editor.fontFamily"
	],
	"editor.fontFamily": "Hack Nerd Font",
	"debug.console.fontFamily": "Hack Nerd Font",
	"gitlens.views.worktrees.showBranchComparison": "branch",
	"vscode-neovim.neovimExecutablePaths.darwin": "nvim",
	"diffEditor.ignoreTrimWhitespace": true,
	"git.autofetch": true,
	"window.zoomLevel": 1,
	"cursor.cpp.disabledLanguages": [
		"plaintext",
		"markdown",
		"scminput"
	],
	"githubPullRequests.pullBranch": "never",
	"editor.formatOnType": true,
	"prisma.showPrismaDataPlatformNotification": false,
	"github.copilot.editor.enableAutoCompletions": true,
	"github.copilot.enable": {
		"*": true,
		"plaintext": false,
		"markdown": false,
		"scminput": false
	},
	"workbench.editor.customLabels.patterns": {
		"**/{page,layout,template,route,actions,index,hooks,components,utils,types}.{js,ts,jsx,tsx}": "${dirname}/${filename}.${extname} .../${dirname(1)}"
	},
	"workbench.colorTheme": "Default Dark Modern",
	"editor.formatOnPaste": true,
	"biome_lsp.trace.server": "verbose",
	"git.blame.editorDecoration.enabled": true,
	"cursor.cpp.enablePartialAccepts": true,
	"editor.cursorSurroundingLines": 1,
	"editor.cursorSurroundingLinesStyle": "all",
	"diffEditor.experimental.showMoves": true,
	"window.commandCenter": true,
	"workbench.layoutControl.enabled": false,
	"typescript.locale": "en",
	"editor.minimap.enabled": true,
	"typescript.inlayHints.parameterNames.enabled": "literals",
	"typescript.inlayHints.parameterTypes.enabled": true,
	"javascript.inlayHints.parameterTypes.enabled": true,
	"vscode-neovim.useWSL": false,
	"vscode-neovim.compositeTimeout": 200,
	"vscode-neovim.compositeKeys": {
		"jj": {
			"command": "vscode-neovim.escape"
		}
	},
	"editor.largeFileOptimizations": false,
	"[json]": {
		"editor.defaultFormatter": "biomejs.biome"
	},
	"cursor.general.disableHttp2": true
}
